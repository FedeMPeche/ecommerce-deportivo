{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "rootDir": "./src",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "typeRoots": ["./node_modules/@types", "./types"],
    "types": ["node", "express"]
  },
  "include": ["src"],
  "exclude": ["node_modules"]

  
}

